{
	"name": "Raw_to_Master",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_raw_parquet",
						"type": "DatasetReference"
					},
					"name": "RAW"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_master_parquet",
						"type": "DatasetReference"
					},
					"name": "LoadMaster",
					"rejectedDataLinkedService": {
						"referenceName": "ADLS_LinkedService_Grupo1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "cast1"
				},
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(output(",
				"          RPJ as string,",
				"          TipoEmpresa as string,",
				"          TipoSector as string,",
				"          NombreEmpresa as string,",
				"          RUC as string,",
				"          CIIU as string,",
				"          Ejercicio as string,",
				"          TipoInformacion as string,",
				"          Trimestre as string,",
				"          Moneda as string,",
				"          MetodoFlujoEfectivo as string,",
				"          ActivoTotal as string,",
				"          PatrimonioTotal as string,",
				"          TotalIngreso as string,",
				"          UtilidadNeta as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> RAW",
				"RAW cast(output(",
				"          RPJ as string,",
				"          TipoEmpresa as string,",
				"          TipoSector as string,",
				"          NombreEmpresa as string,",
				"          RUC as string,",
				"          CIIU as string,",
				"          Ejercicio as string,",
				"          TipoInformacion as string,",
				"          Trimestre as string,",
				"          Moneda as string,",
				"          MetodoFlujoEfectivo as string,",
				"          ActivoTotal as double '000,000,000.00',",
				"          PatrimonioTotal as double '000,000,000.00',",
				"          TotalIngreso as double '000,000,000.00',",
				"          UtilidadNeta as double '000,000,000.00'",
				"     ),",
				"     errors: false) ~> cast1",
				"cast1 derive(UtilidadNeta = toDouble(toString(UtilidadNeta)),",
				"          TotalIngreso = toDouble(toString(TotalIngreso)),",
				"          PatrimonioTotal = toDouble(toString(PatrimonioTotal)),",
				"          ActivoTotal = toDouble(toString(ActivoTotal))) ~> ModifyColumns1",
				"ModifyColumns1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> LoadMaster"
			]
		}
	}
}